using System;

namespace Atividade_4__M
{
	class Program
	{
		public static void Main(string[] args)
		{
			
			Console.WriteLine("Você é um membro ativo no clube? ");
			string resposta = Convert.ToString(Console.ReadLine().ToLower());
			
			if (resposta == "não" || resposta == "n" || resposta == "nao") {
				Console.WriteLine("Por favor, atualize sua inscrição para continuar usufruindo dos benefícios do clube");
			}else if (resposta == "sim" || resposta == "s") {
				
			}
			Console.Write("Press any key to continue . . . ");	
			Console.ReadKey(true);
		}
	}
}
